{
  "name": "opsminded/graph",
  "description": "A simple PHP graph library",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {"Opsminded\\Graph\\": "src/"}
  },
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.7",
    "friendsofphp/php-cs-fixer": "^3.0"
  }
  ,
  "scripts": {
    "cs:check": "phpcs --standard=phpcs.xml src",
    "cs:fix": "php-cs-fixer fix --config=.php-cs-fixer.dist.php src --allow-risky=yes"
  }
}
